{
	"name": "domodel-diary-extension",
	"version": "1.6.85",
	"type": "module",
	"description": "Browser extension context for domodel-diary",
	"scripts": {
		"generate-docs": "jsdoc --configure .jsdoc.json --verbose",
		"start": "rollup --config rollup.config.js --environment BUILD:development",
		"build": "rollup --config rollup.config.js --environment BUILD:production",
		"preversion": "node -e \"const m = require('./public/manifest.json'); m.version = process.env.npm_new_version; const fs = require('fs'); fs.accessSync('./public/manifest.json'); fs.writeFileSync('./public/manifest.json', JSON.stringify(m, null, '\t'), 'utf8')\" && git add public/manifest.json",
		"test": "mocha"
	},
	"author": "Romain Lebesle <oss@thoughtsunificator.me> (https://thoughtsunificator.me)",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@domodel/paginator": "^1.0.10",
		"@domodel/popup": "^1.0.11",
		"@rollup/plugin-alias": "^4.0.2",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@wwa/rollup-plugin-terser": "^1.0.1",
		"concurrently": "^7.6.0",
		"css-generics": "^1.0.4",
		"docdash": "^2.0.0",
		"domodel": "^1.0.46",
		"domodel-diary": "file:../domodel-diary",
		"eslint": "^8.30.0",
		"http-server": "^14.1.1",
		"jsdoc": "^4.0.0",
		"mocha": "^10.2.0",
		"postcss": "^8.4.20",
		"postcss-import": "^15.1.0",
		"puppeteer": "^19.4.1",
		"rollup": "^3.7.5",
		"rollup-plugin-copy-watch": "^0.0.1",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-glob-import": "^0.5.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-postcss": "^4.0.2",
		"wait-on": "^7.0.1",
		"web-ext": "^7.4.0"
	},
	"dependencies": {
		"@domodel/paginator": "^1.0.10",
		"@domodel/popup": "^1.0.11",
		"crypto-es": "^1.2.7",
		"css-generics": "^1.0.4",
		"domodel": "^1.0.46",
		"domodel-diary": "file:../domodel-diary"
	}
}
