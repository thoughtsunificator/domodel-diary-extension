{
	"name": "domodel-diary-extension",
	"version": "1.6.80",
	"type": "module",
	"description": "Browser extension context for domodel-diary",
	"scripts": {
		"start": "rollup --config rollup.config.dev.js",
		"build": "rollup --config rollup.config.prod.js",
		"preversion": "node -e \"const m = require('./public/manifest.json'); m.version = process.env.npm_new_version; const fs = require('fs'); fs.accessSync('./public/manifest.json'); fs.writeFileSync('./public/manifest.json', JSON.stringify(m, null, '\t'), 'utf8')\" && git add public/manifest.json",
		"test": "mocha"
	},
	"author": "Romain Lebesle <oss@thoughtsunificator.me> (https://thoughtsunificator.me)",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"concurrently": "^6.2.1",
		"eslint": "^7.0.0",
		"http-server": "^13.0.2",
		"mocha": "^9.1.1",
		"postcss": "^8.3.6",
		"postcss-import": "^14.0.2",
		"puppeteer": "^10.2.0",
		"rollup": "^2.50.4",
		"rollup-plugin-copy-watch": "^0.0.1",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-root-import": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"wait-on": "^6.0.0",
		"web-ext": "^6.4.0"
	},
	"dependencies": {
		"@domodel/paginator": "^1.0.4",
		"@domodel/popup": "^1.0.5",
		"crypto-es": "^1.2.7",
		"css-generics": "^1.0.0",
		"domodel": "^1.0.7",
		"domodel-diary": "latest"
	}
}
